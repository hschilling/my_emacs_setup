;; -*- emacs-lisp -*- for use by the `custom' package.
;; manual editing is not a good idea.

(custom-set-variables
 '(hs-save-state-option nil))

(custom-set-faces
 '(font-lock-comment-face ((t (:foreground "moccasin"))))
 '(font-lock-string-face ((t (:foreground "orange"))))
 '(font-lock-type-face ((((class color) (background dark)) (:foreground "yellow"))))
 '(font-lock-doc-string-face ((t (:foreground "yellow"))) t)
 '(font-lock-function-name-face ((((class color) (background dark)) (:bold t :foreground "LightSkyBlue")))))
